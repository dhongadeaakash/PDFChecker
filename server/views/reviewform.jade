h4.mdl-color-text--black Review Form
form(action='/submitreview/'+val._id,method='post')
	.mdl-grid
		.mdl-cell.mdl-cell--6-col
			.mdl-textfield.mdl-js-textfield
				label.mdl-textfield__label(for='Creativity') 
				| Creativity & Originality
				input.mdl-slider.mdl-js-slider.mdl-badge(id="CreativityValue"+'#{val._id}' type='range', min='0', max='100', value='0', tabindex='0' oninput='showMessageCC(this.value)', onchange='showMessageCC(this.value)' data-badge='0' name='Creativity' required)
				script.
					function showMessageCC(value){
						document.getElementById("CreativityValue"+"#{val._id}").setAttribute("data-badge",value);
					}
		.mdl-cell.mdl-cell--6-col
			.mdl-textfield.mdl-js-textfield
